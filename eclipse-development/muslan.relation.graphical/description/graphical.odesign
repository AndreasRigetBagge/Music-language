<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/diagram/description/tool/1.1.0" xmlns:tool_1="http://www.eclipse.org/sirius/description/tool/1.1.0" name="graphical" version="12.0.0.2017041100">
  <ownedViewpoints name="Collection Relation Viewpoint" modelFileExtension="relation">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="CollectionRelationDiagramRepresentation" domainClass="relation::RelationRoot" enablePopupBars="true">
      <defaultLayer name="Default">
        <nodeMappings name="MainCollectionNode" label="Main Collection" labelDirectEdit="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@toolSections.0/@ownedTools[name='ChangeNameField']" domainClass="relation::MainCollection">
          <style xsi:type="style:SquareDescription" borderSizeComputationExpression="2" labelSize="12" showIcon="false" tooltipExpression="Create a main collection" labelPosition="node" resizeKind="NSEW" width="20" height="10">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_blue']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_blue']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="DerivedCollectionNode" label="Derived Collected" labelDirectEdit="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@toolSections.0/@ownedTools[name='ChangeNameField']" semanticCandidatesExpression="[ eAllContents()->select(e | not e.oclIsTypeOf(MainCollection)) /]" domainClass="relation::DerivedCollection">
          <style xsi:type="style:BundledImageDescription" borderSizeComputationExpression="2" labelSize="9" showIcon="false" tooltipExpression="Create a derived collection" sizeComputationExpression="10" labelPosition="node" resizeKind="NSEW" shape="ring">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_orange']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_orange']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="CollectionReferenceNode" label="Collection Reference" domainClass="relation::CollectionRerefence">
          <style xsi:type="style:BundledImageDescription" borderSizeComputationExpression="2" labelSize="9" showIcon="false" labelExpression="[ if self.pointsTo &lt;> null then self.pointsTo.Name else '' endif /]" tooltipExpression="Create a collection reference that points to one of your already existing collections! \o/" sizeComputationExpression="10" labelPosition="node" resizeKind="NSEW" shape="ring">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_purple']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_purple']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="SourceNode" label="Source" labelDirectEdit="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@toolSections.0/@ownedTools[name='ChangeNameField']" domainClass="relation::Source">
          <style xsi:type="style:LozengeNodeDescription" borderSizeComputationExpression="2" labelSize="9" showIcon="false" tooltipExpression="Create a new source entity" labelPosition="node" resizeKind="NSEW" widthComputationExpression="8" heightComputationExpression="10">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_green']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_green']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="LocationNode" label="Location" domainClass="relation::Location">
          <style xsi:type="style:LozengeNodeDescription" borderSizeComputationExpression="2" borderLineStyle="dash" labelSize="9" showIcon="false" labelExpression="[if self.track &lt;> null then 'Location of \'' + self.track.Name + '\'' else 'No Track selected' endif /]" tooltipExpression="Create a new track location" sizeComputationExpression="6" labelPosition="node" resizeKind="NSEW" widthComputationExpression="6" heightComputationExpression="8">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='green']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="LinkEdge" label="Link" sourceMapping="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@nodeMappings[name='CollectionReferenceNode'] //@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@nodeMappings[name='DerivedCollectionNode']" targetMapping="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@nodeMappings[name='DerivedCollectionNode'] //@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@nodeMappings[name='MainCollectionNode']" targetFinderExpression="[ self.outgoing /]" sourceFinderExpression="[ self.incoming /]" domainClass="relation::Link" useDomainElement="true">
          <style targetArrow="InputClosedArrow" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='purple']"/>
          </style>
        </edgeMappings>
        <edgeMappings name="SourcesRelationEdge" label="SourcesRelation" sourceMapping="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@nodeMappings[name='MainCollectionNode']" targetMapping="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@nodeMappings[name='SourceNode']" targetFinderExpression="feature:sources">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='green']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="SourceToLocationEdge" label="SourceToLocation" sourceMapping="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@nodeMappings[name='SourceNode']" targetMapping="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@nodeMappings[name='LocationNode']" targetFinderExpression="feature:locations">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_green']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <toolSections name="CreateSection">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="MainCollectionCreationTool" label="Main Collection Creator" nodeMappings="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@nodeMappings[name='MainCollectionNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:CreateInstance" typeName="relation::MainCollection" referenceName="maincollection"/>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="DerivedCollectionCreationTool" label="Derived Collection Creator" nodeMappings="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@nodeMappings[name='DerivedCollectionNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:CreateInstance" typeName="relation::DerivedCollection" referenceName="providers"/>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="LocationCreationTool" label="Location Creator" nodeMappings="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@nodeMappings[name='LocationNode']">
            <variable name="container">
              <subVariables xsi:type="tool_1:SelectModelElementVariable" name="selectedSource" candidatesExpression="[ eAllContents()->select(c | c.oclIsTypeOf(Source))/]"/>
            </variable>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="[ selectedSource /]">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="relation::Location" referenceName="locations"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="CollectionReferenceCreationTool" label="Collection Reference Creator" nodeMappings="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@nodeMappings[name='CollectionReferenceNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:CreateInstance" typeName="relation::CollectionRerefence" referenceName="providers"/>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="SourceCreationTool" label="Source Creator" nodeMappings="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@nodeMappings[name='SourceNode']">
            <variable name="container">
              <subVariables xsi:type="tool_1:SelectModelElementVariable" name="mc" candidatesExpression="[ eAllContents()->select(c | c.oclIsTypeOf(MainCollection) ) /]"/>
            </variable>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="[ mc /]">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="relation::Source" referenceName="sources">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="new source"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:DirectEditLabel" name="ChangeNameField" label="Rename">
            <mask mask="{0}"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="var:0"/>
            </initialOperation>
          </ownedTools>
        </toolSections>
        <toolSections name="ArrowSection" label="Arrow Section">
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="LinkCreationTool" label="Link Creator" edgeMappings="//@ownedViewpoints[name='Collection%20Relation%20Viewpoint']/@ownedRepresentations[name='CollectionRelationDiagramRepresentation']/@defaultLayer/@edgeMappings[name='LinkEdge']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="[ self.eContainer() /]">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="relation::Link" referenceName="links">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="incoming" valueExpression="var:source"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="outgoing" valueExpression="var:target"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="muslan.relation.graphical.Services"/>
  </ownedViewpoints>
</description:Group>
